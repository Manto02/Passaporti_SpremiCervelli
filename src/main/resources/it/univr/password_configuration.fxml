   <children>
      <PasswordField fx:id="nuova_password" layoutX="215.0" layoutY="97.0" promptText="Nuova password" />
      <PasswordField fx:id="conferma_password" layoutX="215.0" layoutY="154.0" promptText="Conferma password" />
      <Button fx:id="conferma" layoutX="260.0" layoutY="250.0" mnemonicParsing="false" onAction="#onConfirmation" text="Conferma" />
      <Label fx:id="success" layoutX="178.0" layoutY="200.0" text="Registarzione avvenuta con successo" textFill="#03ff32" visible="false" />
   </children>
</AnchorPane>
